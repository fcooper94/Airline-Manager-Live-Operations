async function seedHistoricalAircraft() {
  try {
    console.log('=== Historical Aircraft Database Seeding ===\n');

    await sequelize.authenticate();
    console.log('✓ Database connected\n');

    console.log(`Adding ${HISTORICAL_AIRCRAFT.length} aircraft from 1950-present...\n`);

    let added = 0;
    let updated = 0;

    for (const aircraftData of HISTORICAL_AIRCRAFT) {
      const fullName = aircraftData.variant
        ? `${aircraftData.manufacturer} ${aircraftData.model}-${aircraftData.variant}`
        : `${aircraftData.manufacturer} ${aircraftData.model}`;

      // Check if aircraft already exists
      const existing = await Aircraft.findOne({
        where: {
          manufacturer: aircraftData.manufacturer,
          model: aircraftData.model,
          variant: aircraftData.variant
        }
      });

      if (existing) {
        await existing.update(aircraftData);
        console.log(`↻ Updated: ${fullName} (${aircraftData.firstIntroduced})`);
        updated++;
      } else {
        await Aircraft.create(aircraftData);
        console.log(`✓ Added: ${fullName} (${aircraftData.firstIntroduced})`);
        added++;
      }
    }

    console.log('\n=== Summary ===');
    console.log(`Added: ${added}`);
    console.log(`Updated: ${updated}`);
    console.log(`Total: ${HISTORICAL_AIRCRAFT.length}`);

    // Statistics
    const stats = {
      byType: {},
      byEra: {},
      retired: HISTORICAL_AIRCRAFT.filter(a => a.availableUntil !== null).length
    };

    const types = ['Narrowbody', 'Widebody', 'Regional', 'Cargo'];
    for (const type of types) {
      stats.byType[type] = HISTORICAL_AIRCRAFT.filter(a => a.type === type).length;
    }

    const eras = {
      '1950s-1960s': HISTORICAL_AIRCRAFT.filter(a => a.firstIntroduced >= 1950 && a.firstIntroduced < 1970).length,
      '1970s-1980s': HISTORICAL_AIRCRAFT.filter(a => a.firstIntroduced >= 1970 && a.firstIntroduced < 1990).length,
      '1990s-2000s': HISTORICAL_AIRCRAFT.filter(a => a.firstIntroduced >= 1990 && a.firstIntroduced < 2010).length,
      '2010s-Present': HISTORICAL_AIRCRAFT.filter(a => a.firstIntroduced >= 2010).length
    };

    console.log('\nBy Type:');
    Object.keys(stats.byType).forEach(type => {
      console.log(`  ${type}: ${stats.byType[type]}`);
    });

    console.log('\nBy Era:');
    Object.keys(eras).forEach(era => {
      console.log(`  ${era}: ${eras[era]}`);
    });

    console.log(`\nRetired Aircraft: ${stats.retired}`);
    console.log(`Currently Available: ${HISTORICAL_AIRCRAFT.length - stats.retired}`);

    console.log('\n✓ Historical aircraft seeded successfully!');
    console.log('\nAll aircraft enabled (isActive: true) - availableFrom/availableUntil control world availability');

    process.exit(0);
  } catch (error) {
    console.error('\n✗ Seeding failed:', error);
    process.exit(1);
  }
}

seedHistoricalAircraft();
